{
  "name": "jenga-seo",
  "version": "1.0.0",
  "description": "SEO template generator for SPAs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "jenga-seo": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "prepare": "npm run build"
  },
  "keywords": [
    "seo",
    "spa",
    "static",
    "generator",
    "template",
    "html",
    "search",
    "optimization"
  ],
  "author": "Maye Edwin",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "jest": "^29.7.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.9.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mayeedwin/jenga-seo.git"
  },
  "bugs": {
    "url": "https://github.com/mayeedwin/jenga-seo/issues"
  },
  "homepage": "https://github.com/mayeedwin/jenga-seo#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
